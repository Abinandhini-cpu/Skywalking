type AlarmItem {
    content: String!
    startTime: String!
    alertType: AlarmType!
    causeType: CauseType!
}

enum AlarmType {
    APPLICATION,
    SERVER,
    SERVICE
}

enum CauseType {
    LOW_SUCCESS_RATE,
    SLOW_RESPONSE
}

extend type Query {
    loadAlertList(keyword: String, alertType: AlarmType, duration:Duration!, paging: Pagination!):[AlarmItem!]!
}